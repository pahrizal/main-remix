{
  "name": "remix-template-express-ts",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:css": "npm run generate:css -- --minify",
    "dev": "remix build && run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:node": "cross-env NODE_ENV=development nodemon ./build/index.js --watch ./build/index.js",
    "dev:remix": "remix watch",
    "generate:css": "npx tailwindcss -o ./app/assets/styles/tailwind.css",
    "postinstall": "remix setup node",
    "start": "npm run build && cross-env NODE_ENV=production node ./build/index.js"
  },
  "dependencies": {
    "@remix-run/express": "^1.3.2",
    "@remix-run/react": "^1.3.2",
    "clsx": "^1.1.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "reflect-metadata": "^0.1.13",
    "remix": "^1.3.2",
    "socket-controllers": "^0.0.5",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.2",
    "@remix-run/eslint-config": "^1.3.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/uuid": "^8.3.4",
    "eslint": "^8.11.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "redux-devtools-extension": "^2.13.9",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
